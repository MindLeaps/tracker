<table class="table table-striped table-hover resource-table students-table">
  <thead>
    <tr>
      <th class="text-xs-center">MLID</th>
      <th class="text-xs-center">Image</th>
      <th>Last Name</th>
      <th>First Name</th>
      <th class="text-xs-center">Gender</th>
      <th>Date of Birth</th>
      <th class="text-xs-center">Group</th>
    </tr>
  </thead>
  <% students.each do |student| %>
    <%= render partial: "shared/student_row", locals: {student: student} %>
  <% end %>
</table>
